{
  "name": "cmslz/hyperf-tenancy",
  "description": "hyperf多租户",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf"
  ],
  "suggest": {
    "ext-redis": "Required to use Redis Client."
  },
  "autoload": {
    "psr-4": {
      "Cmslz\\HyperfTenancy\\": "src/"
    },
    "files": [
      "src/function.php"
    ]
  },
  "authors": [
    {
      "name": "heqiangchun"
    }
  ],
  "require": {
    "php": ">=8.1",
    "hyperf/engine": "^2.10",
    "hyperf/coroutine": "^3.1",
    "hyperf/logger": "~3.1.0",
    "hyperf/http-server": "~3.1.0",
    "hyperf/di": "^3.1",
    "hyperf/cache": "~3.1.0",
    "guzzlehttp/guzzle": "^7.9",
    "hyperf/paginator": "^3.1",
    "hyperf/resource": "^3.1",
    "hyperf/framework": "~3.1.0",
    "hyperf/database": "~3.1.0",
    "hyperf/db-connection": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/redis": "~3.1.0",
    "hyperf/model-cache": "^3.1",
    "hyperf/amqp": "~3.1.0",
    "hyperf/async-queue": "^3.1.0"
  },
  "require-dev": {
    "swoole/ide-helper": "^5.0"
  },
  "extra": {
    "hyperf": {
      "config": "Cmslz\\HyperfTenancy\\TenancyConfigProvider"
    }
  }
}
